        $('bookCover_13496').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13496').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13496').observe('prototip:hidden', function () {
