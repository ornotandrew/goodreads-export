        $('bookCover_13079982').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13079982').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
