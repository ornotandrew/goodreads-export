<div id="review_3627724082" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3627724082" />
    <a title="Luís" class="left imgcol" href="/user/show/117399210-lu-s"><img alt="Luís" src="https://images.gr-assets.com/users/1636480820p2/117399210.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3627724082?book_show_action=true">Nov 03, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Luís" class="user" itemprop="url" name="Luís" href="/user/show/117399210-lu-s">Luís</a>
