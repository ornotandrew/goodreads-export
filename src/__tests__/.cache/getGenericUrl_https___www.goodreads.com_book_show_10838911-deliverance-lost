        $('bookCover_22599273').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22599273').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
